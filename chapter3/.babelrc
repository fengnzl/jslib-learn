{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": "last 2 versions, > 1%, ie >= 8, Chrome >= 45, safari >= 10",
          "node": "0.12"
        },
        "modules": false,
        "loose": false
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": 3,
        "helpers": false,
        "regenerator": false
      }
    ]
  ],
  "exclude": "node_modules/**"
}
